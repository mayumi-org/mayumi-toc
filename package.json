{
  "name": "@mayumi-org/toc",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://github.com/mayumi-org/mayumi-toc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayumi-org/mayumi-toc.git"
  },
  "bugs": {
    "url": "https://github.com/mayumi-org/mayumi-toc/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && rollup -c --silent",
    "play": "pnpm --filter=./playground dev",
    "dev": "rollup -c -w --silent",
    "test": "echo test",
    "pretest": "pnpm run build",
    "release": "pnpm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "mayumi": "^0.0.3"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@aiou/eslint-config": "0.5.1",
    "@changesets/cli": "2.21.1",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@types/node": "17.0.22",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "cz-emoji": "1.3.1",
    "eslint": "8.11.0",
    "esrua": "0.2.1",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "mayumi": "^0.0.3",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.71.1",
    "rollup-plugin-condition-exports": "1.3.1",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-node-externals": "4.0.0",
    "rollup-plugin-typescript2": "0.31.2",
    "ts-node": "^10.7.0",
    "tslib": "2.4.0",
    "ttypescript": "1.5.13",
    "typescript": "4.4.3",
    "typescript-transform-paths": "3.3.1",
    "vitest": "0.10.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji"
    }
  }
}
